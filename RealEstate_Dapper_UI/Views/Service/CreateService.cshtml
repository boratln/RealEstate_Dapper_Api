@model RealEstate_Dapper_UI.Dtos.WhoWeAreDto.CreateServiceDto
@{
    ViewData["Title"] = "CreateService";
    Layout = "~/Views/Admin/Index.cshtml";
}


<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">

                <h6 class="mb-4">Hizmet Ekleme İşlemi</h6>
                <form method="post">
                    <label>Hizmet Başlığı</label>
                    <input type="text" asp-for="ServiceName" class="form-control" />
                   
                    <br />
                    <button class="btn btn-primary" style="border-radius:8px"> Ekle</button>
                </form>
            </div>
        </div>
    </div>
</div>

